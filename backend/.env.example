# FinClick.AI Environment Variables
# Copy this file to .env and fill in your actual values

MONGO_URL=mongodb://localhost:27017
DB_NAME=finclick_database
CORS_ORIGINS=*

# AI APIs
OPENAI_API_KEY=your-openai-api-key-here
GEMINI_API_KEY=your-gemini-api-key-here
FMP_API_KEY=your-financial-modeling-prep-api-key-here

# Security
JWT_SECRET=your-jwt-secret-key-here

# Database (Optional - for cloud deployment)
SUPABASE_URL=your-supabase-url-here
SUPABASE_ANON_KEY=your-supabase-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key-here

# Additional Financial APIs (Optional)
ALPHA_VANTAGE_API_KEY=your-alpha-vantage-key-here
YAHOO_FINANCE_API_KEY=your-yahoo-finance-key-here
FRED_API_KEY=your-fred-api-key-here
WORLD_BANK_API_KEY=your-world-bank-key-here

# Middle East Financial APIs (Optional)
TADAWUL_API_KEY=your-tadawul-api-key-here
DFM_API_KEY=your-dfm-api-key-here
SAMA_API_KEY=your-sama-api-key-here